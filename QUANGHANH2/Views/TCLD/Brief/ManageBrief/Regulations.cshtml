                 dataTable.ajax.reload();
            });
        });


        function getDeleteIdNganh(id) {
            delete_id_nganh = id.toString();
        }

        function deleteChuyenNganh() {
                $.ajax({
                    type: "POST",
                    url: '@Url.Action("deleteChuyenNganh", "BriefRegulation")',
                    data: {id : delete_id_nganh},
                    success: function (data) {
                        if (data.success)
                        {
                            dataTable.ajax.reload();
                            $.notify(data.message, {
                                globalPosition: "top center",
                                className: "success"
                            });
                            location.reload();

                        }
                    }


                });

        }
        function SubmitForm(form) {
            $.validator.unobtrusive.parse(form);
            if ($(this).valid()) {
                $.ajax({
                    url: form.action,
                    type: "POST",
                    data: $(form).serialize(),
                    success: () => {
                        dataTable.ajax.reload();
                        $.notify(data.message, {
                            globalPosition: "top center",
                            className: "success"
                        });
                    },
                })
            }
            return false;
        }
        function getDeleteId(id) {
            delete_id = id;
        }
        function deleteTruong() {
            if (delete_id != null) {
                $.ajax({
                    type: "POST",
                    url: '@Url.Action("deleteTruong", "BriefRegulation")/' + delete_id,
                    success: function (data) {
                        if (data.success)
                        {
                            dataTableTruong.ajax.reload();
                            $.notify(data.message, {
                                globalPosition: "top center",
                                className: "success"
                            });

                        }
                    }

                });
            }

        }

        function deleteNganh() {
            if (delete_id != null) {
                $.ajax({
                    type: "POST",
                    url: '@Url.Action("deleteNganh", "BriefRegulation")/' + delete_id,
                    success: function (data) {
                        if (data.success)
                        {
                            dataTableNganh.ajax.reload();
                            dataTable.ajax.reload();
                            $.notify(data.message, {
                                globalPosition: "top center",
                                className: "success"
                            });

                        }
                    }

                });
            }

        }

        function getEditChuyenNganh(id) {
            if (id != null) {
                $.ajax({
