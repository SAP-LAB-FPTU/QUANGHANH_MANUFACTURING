        function replace(string) {
            if (string.indexOf('\r\n') != -1) {
                string.replace('\r\n', "");
            }
            if (string.indexOf('\r\n') != -1) {
                string.replace('\r', "");
            }
            if (string.indexOf('\r\n') != -1) {
