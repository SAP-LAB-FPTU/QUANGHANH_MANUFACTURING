
@{
    ViewData["Title"] = "Regulations";
    Layout = "~/Views/Shared/_Layout_TCLD.cshtml";
}
<link href="~/css/advancedsearch/advancedsearch.css" rel="stylesheet" />
<link href="~/assets/Custom css/form-input-border.css" rel="stylesheet" />

<link href="~/assets/Custom css/TCLD/Brief/ManageBrief/beo_regulations.css" rel="stylesheet" />
<div class="card">
    <div class="card-content">
        <div class="row">
            <div class="col l12 m12 s12">
                <h3 class="center"><b>DANH SÁCH QUY ĐỊNH</b></h3>
                <hr>
            </div>
        </div>

        <div class="row">
            <div class="col s12 m12 l12">
                <ul class="tabs">
                    <li class="tab col m3 l3 s3"><a href="#thongtincuatruong">Trường Học</a></li>
                    <li class="tab col m3 l3 s3"><a href="#thongtintrinhdo">Trình Độ</a></li>
                    <li class="tab col m3 l3 s3"><a href="#thongtincuachuyennganh">Chuyên Ngành</a></li>
                    <li class="tab col m3 l3 s3"><a href="#thongtincuanganh">Ngành</a></li>
                </ul>
            </div>
        </div>
        <div id="thongtincuachuyennganh">
            <div class="row">
                <div class="card">
                    <div class="card-content">
                        <div class="row">
                            <div class="col l5 m5 s12 input-field">
                                <input type="text" placeholder="Tìm kiếm bằng tên" class='form-control' id="searchTenCN" multiple />
                            </div>
                            <div class="col l5 m5 s12 input-field">
                                <select class="form-control" id="searchCapBac">
                                    <option value="-1">Chọn Cấp Bậc</option>
                                    @{
                                        foreach (var cb in ViewBag.List_CapBac)
                                        {
                                            <option id="@cb.CapBac">@cb.CapBac</option>
                                        }
                                    }
                                </select>
                            </div>
                            <div class="col l2 m2 s12 input-field">
                                <button class="btn blue darken-2 btn-small" onclick="timKiem()" id="searchButton">Tim Kiếm</button>
                            </div>
                        </div>
                        <div class="row">
                            <a class="btn btn-small blue darken-2 modal-trigger btn-mg" data-target="them_chuyennganh">Thêm thông tin chuyên ngành</a>
                        </div>
                        <table class="table table-bordered table-responsive centered highlight" id="thongtinchuyennganh">
                            <thead>
                                <tr>
                                    <th>Mã Chuyên Ngành</th>
                                    <th>Tên Chuyên Ngành</th>
                                    <th>Cấp Bậc</th>
                                    <th>Chi Tiết</th>
                                    <th>Mã Ngành</th>
                                    <th></th>
                                    <th></th>
                                </tr>
                            </thead>
                            <tbody></tbody>
                        </table>
                        <br />
                    </div>
                </div>
            </div>
        </div>

        <div id="thongtincuanganh">
            <div class="row">
                <div class="card">
                    <div class="card-content">
                        <div class="row">
                            <a class="btn btn-small blue darken-2 modal-trigger btn-mg" data-target="them_nganh">Thêm thông tin ngành</a>
                        </div>
                        <table class="table table-bordered table-responsive centered highlight" id="thongtinnganh">
                            <thead>
                                <tr>
                                    <th>Mã Ngành</th>
                                    <th>Tên Ngành</th>
                                    <th></th>
                                    <th></th>
                                </tr>
                            </thead>
                            <tbody></tbody>
                        </table>
                        <br />
                    </div>
                </div>
            </div>
        </div>

        <div id="thongtincuatruong">
            <div class="row">
                <div class="card">
                    <div class="card-content">
                        <div class="row">
                            <a class="btn btn-small blue darken-2 modal-trigger btn-mg" data-target="them_truong">Thêm thông tin trường</a>
                        </div>
                        <table class="table table-bordered table-responsive centered highlight" id="thongtintruong">
