                                <tr>
                                    <th>Mã Trường</th>
                                    <th>Tên Trường</th>
                                    <th></th>
                                    <th></th>
                                </tr>
                            </thead>
                            <tbody></tbody>
                        </table>
                        <br />
                    </div>
                </div>
            </div>
        </div>

        <div id="thongtintrinhdo">
            <div class="row">
                <div class="card">
                    <div class="card-content">
                        <div class="row">
                            <a class="btn btn-small blue darken-2 modal-trigger btn-mg" data-target="them_trinhdo">Thêm thông tin trình độ</a>
                        </div>
                        <table class="table table-bordered table-responsive centered highlight" id="trinhdo">
                            <thead>
                                <tr>
                                    <th>Mã Trình Độ</th>
                                    <th>Trình Độ</th>
                                    <th></th>
                                    <th></th>
                                </tr>
                            </thead>
                            <tbody></tbody>
                        </table>
                        <br />
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>


<!-- Modal Structure -->
