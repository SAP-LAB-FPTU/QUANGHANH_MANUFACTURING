
@{
    ViewBag.Title = "Input";
    Layout = "~/Views/Shared/_Layout_TCLD.cshtml";
}

<link rel="stylesheet" href="~/css/tuan_css/css/baocaoluong_css/style.css" />
<link rel="stylesheet" href="~/css/tuan_css/css/kcs_css/style.css" />

<div class="card">
    <div class="card-content">
        <div class="container-fluid">
            <div class="row">
                <div class="center">
                    <h3>DANH SÁCH TIẾP NHẬN VÀ BỐ TRÍ CÔNG TÁC ĐỐI VỚI CÔNG NHÂN</h3>
                    <h4>Kèm theo Quyết định số: /QĐ-VQHC</h4>
                    <input type="text"
                           class="datepicker-here center"
                           data-language='vi'
                           data-min-view="days"
                           id="minMaxDate"
                           data-view="days"
                           data-date-format="dd MM yyyy"
                           placeholder="Ngày quyết định" />
                </div>
            </div>
            <br/>
            <div class="card">
                <div class="card-body">
                    <table class="table table-bordered table-responsive">
                        <thead>
                            <tr>
                                <th>STT</th>
                                <th>Số thẻ</th>
                                <th>Họ tên</th>
                                <th>Ngày sinh</th>
                                <th>Đơn vị</th>
                                <th>Loại</th>
                                <th>Trình độ</th>
                                <th>Chuyên Nghành</th>
                                <th>Công việc bố trí</th>
                                <th>Thường trú</th>
                                <th>Thang lương</th>
                                <th>Bậc</th>
                                <th>Mức lương (đồng/tháng)</th>
                            </tr>
                        </thead>
                        <tbody></tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>

