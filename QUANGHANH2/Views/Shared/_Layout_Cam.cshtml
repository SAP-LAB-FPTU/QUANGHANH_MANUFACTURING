    <!-- ============================================================== -->
    <link href="~/lib/datepicker_src/dist/css/datepicker.min.css" rel="stylesheet" type="text/css">
    <link href="~/lib/datepicker_src/css/style.css" rel="stylesheet " type="text/css ">
    <link href="~/lib/datepicker_src/dist/css/datepicker.min.css " rel="stylesheet " type="text/css ">
    <link href="~/lib/datepicker_src/dist/css/custom.css " rel="stylesheet " type="text/css ">
    <link href="~/lib/datepicker_src/dist/css/font.css" rel="stylesheet" type="text/css">
    <script src="~/lib/datepicker_src/dist/js/datepicker.js"></script>
    <script src="~/lib/datepicker_src/dist/js/i18n/datepicker.vi.js"></script>
</head>

<body>
    <div class="main-wrapper" id="main-wrapper">
        <div id="pre-load" hidden>
            <div class="loading">
                <div class="preloader-wrapper big active">
                    <div class="spinner-layer spinner-blue-only">
                        <div class="circle-clipper left">
                            <div class="circle"></div>
                        </div><div class="gap-patch">
                            <div class="circle"></div>
                        </div><div class="circle-clipper right">
                            <div class="circle"></div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- ============================================================== -->
        <!-- Preloader - style you can find in spinners.css -->
        <!-- ============================================================== -->
        <div class="preloader">
            <div class="loading">
                <div class="preloader-wrapper big active">
                    <div class="spinner-layer spinner-blue-only">
                        <div class="circle-clipper left">
                            <div class="circle"></div>
                        </div><div class="gap-patch">
                            <div class="circle"></div>
                        </div><div class="circle-clipper right">
                            <div class="circle"></div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- ============================================================== -->
        <!-- Preloader - style you can find in spinners.css -->
        <!-- ============================================================== -->
        <header class="topbar">
            <!-- ============================================================== -->
            <!-- Navbar scss in header.scss -->
            <!-- ============================================================== -->
            <nav>
                <div class="nav-wrapper">
                    <!-- ============================================================== -->
                    <!-- Logo you can find that scss in header.scss -->
                    <!-- ============================================================== -->
                    <a href="javascript:void(0)" class="brand-logo">
                        <span class="icon">
                            <img class="light-logo" src="/assets/images/logo-light-icon.png">
                            <img class="dark-logo" src="~/assets/images/logo-text.png">

                        </span>
                        <span class="text">
                            <img class="light-logo" src="/assets/images/logo-light-text.png">
                            <img class="dark-logo" src="~/assets/images/logo-text.png">

                        </span>
                    </a>
                    <!-- ============================================================== -->
                    <!-- Logo you can find that scss in header.scss -->
                    <!-- ============================================================== -->
                    <!-- ============================================================== -->
                    <!-- Left topbar icon scss in header.scss -->
                    <!-- ============================================================== -->
                    <ul class="left">
                        <li class="hide-on-med-and-down">
                            <a href="javascript: void(0);" class="nav-toggle">
                                <span class="bars bar1"></span>
                                <span class="bars bar2"></span>
                                <span class="bars bar3"></span>
                            </a>
                        </li>
                        <li class="hide-on-large-only">
                            <a href="javascript: void(0);" class="sidebar-toggle">
                                <span class="bars bar1"></span>
                                <span class="bars bar2"></span>
                                <span class="bars bar3"></span>
                            </a>
                        </li>
                        <!-- ============================================================== -->
                        <!-- Notification icon scss in header.scss -->
                        <!-- ============================================================== -->
                        <li>
                            <a class="dropdown-trigger" href="javascript: void(0);" data-target="noti_dropdown">
                                <i class="small material-icons">notifications</i>
                            </a>
                            <ul id="noti_dropdown" class="mailbox dropdown-content">
                                <li>
                                    <div class="drop-title">Thông Báo</div>
                                </li>
                                <li>
                                    <div class="message-center">
                                        <!-- Message -->
                                        <a href="#">
                                            <span class="btn-floating btn-large red">
                                                <i class="small material-icons">link</i>
                                            </span>
                                            <span class="mail-contnet">
                                                <h5>Phòng CĐVT</h5>
                                                <span class="mail-desc">Cần cập nhật thiết bị</span> <span class="time">9:30 AM</span>
                                            </span>
                                        </a>
                                        <!-- Message -->
                                        <a href="#">
                                            <span class="btn-floating btn-large blue">
                                                <i class="small material-icons">date_range</i>
                                            </span>
                                            <span class="mail-contnet">
                                                <h5>Các sự kiện</h5>
                                                <span class="mail-desc">Đặt lịch họp cho trưởng phòng</span>
                                                <span class="time">9:10 AM</span>
                                            </span>
                                        </a>
                                    </div>
                                </li>
                                <li>
                                    <a class="center-align" href="javascript:void(0);">
                                        <strong>Xoá thông báo</strong>
                                    </a>
                                </li>
                            </ul>
                        </li>
                        <!-- ============================================================== -->
                        <!-- Comment topbar icon scss in header.scss -->
                        <!-- ============================================================== -->
                        <li>
                            <a class="dropdown-trigger" href="javascript: void(0);" data-target="msg_dropdown">
                                <i class="small material-icons">comment</i>
                            </a>
                            <ul id="msg_dropdown" class="mailbox dropdown-content">
                                <li>
                                    <div class="drop-title">Bạn có 4 tin nhắn mới</div>
                                </li>
                                <li>
                                    <div class="message-center">
                                        <!-- Message -->
                                        <a href="#">
                                            <span class="user-img">
                                                <img src="~/assets/images/users/2.jpg" alt="user" class="circle">
                                                <span class="profile-status online pull-right"></span>
                                            </span>
                                            <span class="mail-contnet">
                                                <h5>Trần Long- CĐVT</h5>
                                                <span class="mail-desc">Gửi báo cáo chưa ? </span>
                                                <span class="time">9:30 AM</span>
                                            </span>
                                        </a>
                                        <!-- Message -->
                                        <a href="#">
                                            <span class="user-img">
                                                <img src="~/assets/images/users/2.jpg" alt="user" class="circle">
                                                <span class="profile-status busy pull-right"></span>
                                            </span>
                                            <span class="mail-contnet">
                                                <h5>Nguyễn Văn Học - TCLĐ</h5>
                                                <span class="mail-desc">Gửi anh báo cáo nhân sự</span>
                                                <span class="time">9:10 AM</span>
                                            </span>
                                        </a>
                                        <!-- Message -->
                                        <a href="#">
                                            <span class="user-img">
                                                <img src="~/assets/images/users/2.jpg" alt="user" class="circle">
                                                <span class="profile-status away pull-right"></span>
                                            </span>
                                            <span class="mail-contnet">
                                                <h5>Trần Văn Vương - TCLĐ</h5>
                                                <span class="mail-desc">Em ơi gửi anh chứng chỉ lao động</span>
                                                <span class="time">9:08 AM</span>
                                            </span>
                                        </a>
                                        <!-- Message -->
                                        <a href="#">
                                            <span class="user-img">
                                                <img src="~/assets/images/users/2.jpg" alt="user" class="circle">
                                                <span class="profile-status offline pull-right"></span>
                                            </span>
                                            <span class="mail-contnet">
                                                <h5>Lê Văn Tam - Trưởng phòng CDVT</h5>
                                                <span class="mail-desc">Gửi báo cáo hàng ngày em nhé!</span>
                                                <span class="time">9:02 AM</span>
                                            </span>
                                        </a>
                                    </div>
                                </li>
                                <li>
                                    <a class="center-align" href="javascript:void(0);">
                                        <strong>Xoá tin nhắn</strong>
                                    </a>
                                </li>
                            </ul>
                        </li>
                    </ul>
                    <!-- ============================================================== -->
                    <!-- Left topbar icon scss in header.scss -->
                    <!-- ============================================================== -->
                    <!-- ============================================================== -->
                    <!-- Right topbar icon scss in header.scss -->
                    <!-- ============================================================== -->

                    <ul class="right">
                        <!-- ============================================================== -->
                        <!-- Profile icon scss in header.scss -->
                        <!-- ============================================================== -->
                        <li>
                            <a class="dropdown-trigger" href="javascript: void(0);" data-target="user_dropdown"><img src="/assets/images/users/2.jpg" alt="user" class="circle profile-pic"></a>
                            <ul id="user_dropdown" class="mailbox dropdown-content dropdown-user">
                                <li>
                                    <div class="dw-user-box">
                                        <div class="u-img"><img src="/assets/images/users/2.jpg" alt="user"></div>
                                        <div class="u-text">
                                            <h4>@Session["Name"]</h4>
                                            <p>@Session["username"]</p>
                                            <a class="waves-effect waves-light btn-small red white-text">Xem Hồ Sơ</a>
                                        </div>
                                    </div>
                                </li>
                                <li role="separator" class="divider"></li>
                                <li><a href="#"><i class="small material-icons">account_circle</i> Hồ Sơ Của Tôi</a></li>
                                <li><a href="#"><i class="small material-icons">inbox</i> Tin nhắn</a></li>
                                <li role="separator" class="divider"></li>
                                <li><a href="#"><i class="small material-icons">settings</i> Cài Đặt Tài Khoản</a></li>
                                <li role="separator" class="divider"></li>
                                <li><a href="@Url.Action("Logout", "LogIn")"><i class="material-icons">power_settings_new</i>Đăng Xuất</a></li>
                            </ul>
                        </li>
                        <li id="user-icon"><a href="">@Session["Name"] <span class="new badge red" data-badge-caption="@Session["Position"]"></span></a></li>
                    </ul>
                    <!-- ============================================================== -->
                    <!-- Right topbar icon scss in header.scss -->
                    <!-- ============================================================== -->
                </div>
            </nav>
            <!-- ============================================================== -->
            <!-- Navbar scss in header.scss -->
            <!-- ============================================================== -->
        </header>
        <!-- ============================================================== -->
        <!-- Sidebar scss in sidebar.scss -->
        <!-- ============================================================== -->
        <aside class="left-sidebar">
            <ul id="slide-out" class="sidenav">
                <li>
                    <ul class="collapsible">
                        <li>
                            <a href="/camera" class="collapsible-header"><i class="small material-icons">business_center</i><span class="hide-menu">Danh sách camera</span></a>
                        </li>
                        <li>
                            <a href="/camera/su-co" class="collapsible-header"><i class="small material-icons">build</i><span class="hide-menu">Sự cố</span></a>
                        </li>
                        <li>
                            <a href="/phong-cdvt/cap-nhat/quyet-dinh" class="collapsible-header"><i class="material-icons">autorenew</i><span class="hide-menu">Xử lý quyết định</span></a>
                        </li>
                        <li>
                            <a href="/phong-cdvt/cap-nhat/quyet-dinh" class="collapsible-header"><i class="material-icons">autorenew</i><span class="hide-menu">Nghiệm thu</span></a>
                        </li>
                        <li>
                            <a href="/phong-cdvt/kiem-dinh" class="collapsible-header"><i class="material-icons">assignment_turned_in</i><span class="hide-menu">Quyết định sửa chữa</span></a>
                        </li>

                    </ul>
                </li>
            </ul>
        </aside>
        <!-- ============================================================== -->
        <!-- Sidebar scss in sidebar.scss -->
        <!-- ============================================================== -->
        <!-- ============================================================== -->
        <!-- Page wrapper scss in scafholding.scss -->
        <!-- ============================================================== -->
        <div class="page-wrapper">
            <!-- ============================================================== -->
            <!-- Title and breadcrumb -->
            <!-- ============================================================== -->
            <!-- ============================================================== -->
            <!-- Container fluid scss in scafholding.scss -->
            <!-- ============================================================== -->
            <div class="container-fluid">
                @RenderBody()
                @RenderSection("scripts", required: false)
            </div>
            <!-- ============================================================== -->
            <!-- Container fluid scss in scafholding.scss -->
            <!-- ============================================================== -->
        </div>
        <!-- ============================================================== -->
        <!-- Page wrapper scss in scafholding.scss -->
        <!-- ============================================================== -->
        <!-- ============================================================== -->
        <!-- Right Sidebar -->
        <!-- ============================================================== -->
        <!-- ============================================================== -->
        <!-- Right Sidebar -->
        <!-- ============================================================== -->
    </div>

    <script src="~/assets/sweetalert/alert-quanghanh-js.js"></script>
    <script src="~/assets/sweetalert/sweetalert2.all.min.js"></script>
    <script src="~/assets/sweetalert/sweetalert2.min.js"></script>
    <script src="~/js/Custom JS/Custom_input_required.js"></script>
    <script src="~/Scripts/jquery.validate.min.js"></script>
    <script src="~/Scripts/jquery.validate.unobtrusive.min.js"></script>
    <script src="~/Scripts/notify.min.js"></script>
    <script src="~/js/jquery.dataTables.min.js"></script>
    <script>
        $(".loading").css("z-index", 99999);
        $('body').css('overflow', '');
    </script>
    <script>
        $(".datepicker-here").keydown(function (e) {
            e.preventDefault();
        });
    </script>
</body>

</html>
