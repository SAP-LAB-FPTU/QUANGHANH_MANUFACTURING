        //////////////////////////////////////
        var date = new Date();
        var selectedDate = "";
        selectedDate="@ViewBag.ngay";
        if (selectedDate === "0") {
            $("#ngay-chon").val(date.getDate() + "/" + (date.getMonth() + 1) + "/" + date.getFullYear());
        } else {
            $("#ngay-chon").val(selectedDate);
        }

        $(".datepicker-inline").remove();
