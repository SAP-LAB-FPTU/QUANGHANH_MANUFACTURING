    var listFiles = ViewBag.listFiles;
    var ca1Number = 0;
    var ca2Number = 0;
    var ca3Number = 0;
    for (int i = 0; i < listFiles.Count; i++)
    {
        switch (listFiles[i].ca)
        {
            case 1:
                ca1Number++;
                break;
            case 2:
                ca2Number++;
                break;
            case 3:
                ca3Number++;
                break;
            default:
                break;
        }
    }
}

